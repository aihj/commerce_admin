{
  "name": "pco-admin-frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3010",
    
    "dev-local-local": "env-cmd -f .env.local-local next dev -p 3010",
    "dev-local-live": "env-cmd -f .env.local-live next dev -p 3010",
    "dev-prod-test": "env-cmd -f .env.prod-test next dev -p 3010",
    "dev-prod-live": "env-cmd -f .env.prod-test next dev -p 3010",
    "build-local-test": "env-cmd -f .env.local-test next build",
    "build-local-live": "env-cmd -f .env.local-test next build",
    "build-prod-test": "env-cmd -f .env.prod-test next build",
    "build-prod-live": "env-cmd -f .env.prod-live next build",
    "build:dev": "env-cmd -f .env.development next build",
    "build": "HUSKY=0 next build",
    "prepare": "husky",
    "start": "next start",
    "lint": "next lint",
    "precommit": "lint-staged",
    "orval": "orval",
    "format:write": "prettier --write \"**/*.{js,jsx,mjs,ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{js,jsx,mjs,ts,tsx,mdx}\" --cache",
    "lint:fix": "eslint --fix --ext .ts,.tsx"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --config ./.prettierrc --write -u"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/inter": "^5.0.16",
    "@fontsource/plus-jakarta-sans": "^5.0.18",
    "@fontsource/roboto-mono": "^5.0.16",
    "@hookform/resolvers": "^3.3.4",
    "@mui/icons-material": "^5.16.1",
    "@mui/material": "^5.16.1",
    "@mui/x-date-pickers": "^7.22.1",
    "@phosphor-icons/react": "2.0.15",
    "@reduxjs/toolkit": "^2.1.0",
    "@tanstack/react-query": "^5.28.9",
    "@tanstack/react-table": "^8.15.3",
    "@tiptap/react": "^3.4.2",
    "@tiptap/starter-kit": "^3.4.2",
    "@types/react-table": "^7.7.20",
    "axios": "^1.6.8",
    "class-variance-authority": "^0.7.0",
    "dayjs": "^1.11.10",
    "env-cmd": "^10.1.0",
    "next": "14.1.0",
    "next-auth": "^4.24.7",
    "prettier": "^3.2.4",
    "react": "^18",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.51.0",
    "react-icons": "^5.0.1",
    "react-quill": "^2.0.0",
    "react-redux": "^9.1.0",
    "recharts": "^2.12.3",
    "redux-persist": "^6.0.0",
    "sass": "^1.92.1",
    "sonner": "^1.4.2",
    "sweetalert2": "^11.10.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@hookform/devtools": "^4.3.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "autoprefixer": "^10.0.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^9.0.7",
    "lint-staged": "^15.2.1",
    "msw": "^2.2.13",
    "orval": "^6.25.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.4.3",
    "typescript-eslint": "^7.3.1"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
