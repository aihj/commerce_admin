{
  "containerDefinitions": [
    {
      "name": "pco-prod-admin-front-container",
      "image": "980602070685.dkr.ecr.ap-northeast-2.amazonaws.com/pco-prod-admin-front-ecr:latest",
      "cpu": 0,
      "portMappings": [
        {
          "name": "pco-prod-admin-front-container-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "TZ",
          "value": "Asia/Seoul"
        }
      ],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/pco-prod-admin-front-task",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      }
    }
  ],
  "family": "pco-prod-admin-front-task",
  "executionRoleArn": "arn:aws:iam::980602070685:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072"
}
